{
  "id": "hotdoug",
  "name": "Hot Doug the hotdog",
  "bio": "hot doug.",
  "level": 0,
  "backgroundImage": "",
  "logic": {
    "stats": [
      {
        "id": "age",
        "label": "Age",
        "value": 0.0,
        "perMinute": 0,
        "max": 2,
        "fullIsGood": true,
        "statEffects": [
          { "when": "=_0", "then": "STATUS_AGE_BABY" },
          { "when": "=_1", "then": "STATUS_AGE_ADULT" },
          { "when": "=_2", "then": "STATUS_AGE_COOKED" }
        ]
      },
      {
        "id": "temperature",
        "label": "Internal Temp",
        "value": 50.0,
        "perMinute": -10.0,
        "max": 650.0,
        "fullIsGood": false,
        "statEffects": [
          { "when": "<=_32", "then": "STATUS_TEMP_COLD" },
          { "when": [">=_32", "<_90"], "then": "STATUS_TEMP_NORMAL" },
          { "when": [">=_90", "<_500"], "then": "STATUS_TEMP_HOT" },
          { "when": ">_500", "then": "STATUS_TEMP_BURNING" },
          { "when": ">=_100%", "then": "STATUS_DEAD" }
        ]
      },
      {
        "id": "food",
        "label": "Food",
        "value": 50.0,
        "perMinute": -1,
        "max": 100.0,
        "fullIsGood": true,
        "statEffects": [
          { "when": "<=_25%", "then": "STATUS_FOOD_HUNGRY" },
          { "when": ">_25%", "then": "STATUS_FOOD_FULL" }
        ]
      },
      {
        "id": "awake",
        "label": "Awake",
        "value": 50.0,
        "perMinute": -1,
        "max": 100.0,
        "fullIsGood": true,
        "statEffects": [
          { "when": "<=_25%", "then": "STATUS_SLEEP_ASLEEP" },
          { "when": ">_25%", "then": "STATUS_SLEEP_AWAKE" },
          { "when": ">_75%", "then": "STATUS_SLEEP_ALERT" }
        ]
      },
      {
        "id": "waste",
        "label": "Waste",
        "value": 0.0,
        "perMinute": 1,
        "max": 100.0,
        "fullIsGood": false,
        "statEffects": [
          { "when": "<=_50%", "then": "STATUS_WASTE_CLEAN" },
          { "when": ">_80%", "then": "STATUS_WASTE_DIRTY" }
        ]
      },
      {
        "id": "condiments",
        "label": "Condiments",
        "value": 0.0,
        "perMinute": 0,
        "max": 1.0,
        "fullIsGood": false,
        "statEffects": [
          { "when": "=_0", "then": "STATUS_CLOTHES_NAKED" },
          { "when": "=_1", "then": "STATUS_CLOTHES_DRESSED" }
        ]
      }
    ],
    "statuses": {
      "STATUS_TEMP_COLD": {
        "label": "frozen!",
        "alertType": "warning"
      },
      "STATUS_TEMP_NORMAL": {},
      "STATUS_TEMP_HOT": {
        "label": "cookin'"
      },
      "STATUS_TEMP_BURNING": {
        "label": "burning!",
        "alertType": "alert"
      },
      "STATUS_WASTE_CLEAN": {},
      "STATUS_WASTE_DIRTY": {},
      "STATUS_FOOD_HUNGRY": {
        "label": "hungry!",
        "alertType": "warning"
      },
      "STATUS_FOOD_FULL": {},
      "STATUS_AGE_BABY": {},
      "STATUS_AGE_ADULT": {},
      "STATUS_SLEEP_ASLEEP": {
        "label": "zzzzz.."
      },
      "STATUS_SLEEP_AWAKE": {},
      "STATUS_SLEEP_ALERT": {},
      "STATUS_CLOTHES_NAKED": {},
      "STATUS_CLOTHES_DRESSED": {},
      "STATUS_DEAD": {
        "label": "RIP",
        "alertType": "alert"
      },
      "STATUS_GRILL_ON": {
        "label": "grill ON",
        "changeStats": [
          {
            "statId": "temperature",
            "value": 100.0
          }
        ]
      },
      "STATUS_GRILL_OFF": {
        "label": "grill OFF"
      }
    },
    "behaviorRules": [
      {
        "when": "STATUS_DEAD",
        "then": "BE_DEAD"
      },
      {
        "when": "STATUS_AGE_BABY",
        "then": [
          {
            "when": "STATUS_TEMP_BURNING",
            "then": "BE_BABY_BURNING"
          },
          {
            "when": "STATUS_TEMP_HOT",
            "then": "BE_BABY_COOKING"
          },
          {
            "when": "STATUS_TEMP_COLD",
            "then": "BE_BABY_FROZEN"
          },
          {
            "when": [["STATUS_WASTE_DIRTY", "STATUS_TEMP_COLD", "STATUS_FOOD_HUNGRY"]],
            "then": "BE_BABY_SAD"
          },
          {
            "when": ["STATUS_SLEEP_ASLEEP"],
            "then": "BE_BABY_ASLEEP"
          },
          {
            "then": "BE_BABY_IDLE"
          }
        ]
      },
      {
        "when": "STATUS_AGE_ADULT",
        "then": [
          {
            "when": "STATUS_TEMP_BURNING",
            "then": "BE_RAW_BURNING"
          },
          {
            "when": "STATUS_TEMP_HOT",
            "then": "BE_RAW_COOKING"
          },
          {
            "when": "STATUS_TEMP_COLD",
            "then": "BE_RAW_FROZEN"
          },
          {
            "when": "STATUS_TEMP_NORMAL",
            "then": [
              {
                "when": [["STATUS_FOOD_HUNGRY", "STATUS_WASTE_DIRTY"]],
                "then": "BE_RAW_SAD"
              },
              {
                "when": "STATUS_SLEEP_ASLEEP",
                "then": "BE_RAW_ASLEEP"
              }
            ]
          },
          {
            "then": "BE_RAW_IDLE"
          }
        ]
      },
      {
        "when": "STATUS_AGE_COOKED",
        "then": [
          {
            "when": "STATUS_WASTE_DIRTY",
            "then": [
              {
                "when": "STATUS_SLEEP_ASLEEP",
                "then": "BE_COOKED_DRESSED_SLEEP"
              },
              {
                "then": "BE_COOKED_DRESSED_IDLE"
              }
            ]
          },
          {
            "then": [
              {
                "when": "STATUS_SLEEP_ASLEEP",
                "then": "BE_COOKED_SLEEP"
              },
              {
                "then": "BE_COOKED_IDLE"
              }
            ]
          }
        ]
      },
      {
        "then": "BE_NORMAL"
      }
    ],
    "behaviors": [
      { "id": "BE_NORMAL", "image": "assets/cooked_idle.gif", "position": "bottom", "offsetY": 0 },

      { "id": "BE_BABY_IDLE", "image": "assets/baby_idle.gif", "position": "bottom", "offsetY": 0 },
      { "id": "BE_BABY_SAD", "image": "assets/baby_sad.gif", "position": "bottom", "offsetY": 0 },
      { "id": "BE_BABY_ASLEEP", "image": "assets/baby_sleep.gif", "position": "bottom", "offsetY": 0 },
      {
