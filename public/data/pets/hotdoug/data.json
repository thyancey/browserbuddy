{
  "id": "hotdoug",
  "name": "Hot Doug the hotdog",
  "bio": "hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. hot doug. ",
  "backgroundImage": "",
  "logic": {
    "stats": [
      {
        "id": "level",
        "label": "Level",
        "value": 1,
        "perMinute": 0,
        "max": 3,
        "fullIsGood": true,
        "statEffects": [
          { "when": "=_1", "then": "STATUS_AGE_BABY" },
          { "when": "=_2", "then": "STATUS_AGE_RAW" },
          { "when": "=_3", "then": "STATUS_AGE_COOKED" }
        ]
      },
      {
        "id": "temperature",
        "label": "Internal Temp",
        "value": 50.0,
        "perMinute": -10.0,
        "max": 600.0,
        "fullIsGood": false,
        "statEffects": [
          { "when": "<=_32", "then": "STATUS_TEMP_COLD" },
          { "when": [">=_32", "<_100"], "then": "STATUS_TEMP_NORMAL" },
          { "when": [">=_100", "<_400"], "then": "STATUS_TEMP_HOT" },
          { "when": [">=_200", "<_400"], "then": "STATUS_TEMP_GOLDENBROWN" },
          { "when": ">_400", "then": "STATUS_TEMP_BURNING" },
          { "when": ">=_100%", "then": "STATUS_DEAD" }
        ]
      },
      {
        "id": "awake",
        "label": "Awake",
        "value": 50.0,
        "perMinute": -1,
        "max": 100.0,
        "fullIsGood": true,
        "statEffects": [
          { "when": "<=_25%", "then": "STATUS_SLEEP_ASLEEP" },
          { "when": ">_25%", "then": "STATUS_SLEEP_AWAKE" },
          { "when": ">_75%", "then": "STATUS_SLEEP_ALERT" }
        ]
      },
      {
        "id": "waste",
        "label": "Waste",
        "value": 0.0,
        "perMinute": 10,
        "max": 100.0,
        "fullIsGood": false,
        "statEffects": [
          { "when": "<_25%", "then": "STATUS_WASTE_CLEAN" },
          { "when": "<_75%", "then": "STATUS_WASTE_NORMAL" },
          { "when": ">_75%", "then": "STATUS_WASTE_DIRTY" }
        ]
      }
    ],
    "statuses": {
      "STATUS_TEMP_COLD": {
        "label": "frozen!",
        "alertType": "warning"
      },
      "STATUS_TEMP_NORMAL": {},
      "STATUS_TEMP_HOT": {
        "label": "cookin'"
      },
      "STATUS_TEMP_GOLDENBROWN": {
        "label": "golden brown"
      },
      "STATUS_TEMP_BURNING": {
        "label": "burning!",
        "alertType": "alert"
      },
      "STATUS_WASTE_CLEAN": {},
      "STATUS_WASTE_NORMAL": {},
      "STATUS_WASTE_DIRTY": {
        "label": "filthy!",
        "alertType": "warning"
      },
      "STATUS_AGE_BABY": {},
      "STATUS_AGE_RAW": {},

      "STATUS_SLEEP_ASLEEP": {
        "label": "zzzzz.."
      },
      "STATUS_SLEEP_AWAKE": {},
      "STATUS_SLEEP_ALERT": {},
      
      "STATUS_DEAD": {
        "label": "RIP",
        "alertType": "alert"
      },
      "STATUS_GRILL_ON": {},
      "STATUS_GRILL_OFF": {}
    },
    "behaviorRules": [
      {
        "when": "STATUS_DEAD",
        "then": [
          {
            "when": "STATUS_AGE_BABY",
            "then": "BE_BABY_DEAD"
          },
          {
            "when": "STATUS_AGE_RAW",
            "then": "BE_RAW_DEAD"
          },
          {
            "then": "BE_DEAD"
          }
        ]
      },
      {
        "when": "STATUS_AGE_BABY",
        "then": [
          {
            "when": "STATUS_TEMP_BURNING",
            "then": "BE_BABY_BURNING"
          },
          {
            "when": "STATUS_TEMP_HOT",
            "then": [
              {
                "when": "STATUS_WASTE_DIRTY",
                "then": "BE_BABY_COOKING_SAD"
              },
              {
                "then": "BE_BABY_COOKING"
              }
            ]
          },
          {
            "when": "STATUS_TEMP_COLD",
            "then": "BE_BABY_FROZEN"
          },
          {
            "when": "STATUS_WASTE_DIRTY",
            "then": "BE_BABY_SAD"
          },
          {
            "when": "STATUS_SLEEP_ASLEEP",
            "then": "BE_BABY_ASLEEP"
          },
          {
            "then": "BE_BABY_IDLE"
          }
        ]
      },
      {
        "when": "STATUS_AGE_RAW",
        "then": [
          {
            "when": "STATUS_TEMP_BURNING",
            "then": "BE_RAW_BURNING"
          },
          {
            "when": "STATUS_TEMP_HOT",
            "then": [
              {
                "when": "STATUS_WASTE_DIRTY",
                "then": "BE_RAW_COOKING_SAD"
              },
              {
                "then": "BE_RAW_COOKING"
              }
            ]
          },
          {
            "when": "STATUS_TEMP_COLD",
            "then": "BE_RAW_FROZEN"
          },
          {
            "when": "STATUS_TEMP_NORMAL",
            "then": [
              {
                "when": ["STATUS_WASTE_DIRTY"],
                "then": "BE_RAW_SAD"
              },
              {
                "when": "STATUS_SLEEP_ASLEEP",
                "then": "BE_RAW_ASLEEP"
              }
            ]
          },
          {
            "then": "BE_RAW_IDLE"
          }
        ]
      },
      {
        "when": "STATUS_AGE_COOKED",
        "then": [
          {
            "when": "STATUS_WASTE_DIRTY",
            "then": [
              {
                "when": "STATUS_SLEEP_ASLEEP",
                "then": "BE_COOKED_DRESSED_SLEEP"
              },
              {
                "then": "BE_COOKED_DRESSED_IDLE"
              }
            ]
          },
          {
            "then": [
              {
                "when": "STATUS_SLEEP_ASLEEP",
                "then": "BE_COOKED_SLEEP"
              },
              {
                "then": "BE_COOKED_IDLE"
              }
            ]
          }
        ]
      },
      {
        "then": "BE_NORMAL"
      }
    ],
    "behaviors": [
      { "id": "BE_NORMAL", "image": "assets/cooked_idle.gif", "position": "bottom" },

      { "id": "BE_BABY_IDLE", "image": "assets/baby_idle.gif", "position": "bottom" },
      { "id": "BE_BABY_SAD", "image": "assets/baby_sad.gif", "position": "bottom" },
      { "id": "BE_BABY_ASLEEP", "image": "assets/baby_sleep.gif", "position": "bottom" },
      {
        "id": "BE_BABY_FROZEN",
        "image": "assets/baby_frozen.gif",
        "position": "bottom",
        "backgroundImage": "assets/bg_frozen.gif"
      },
      {
        "id": "BE_BABY_COOKING",
        "image": "assets/baby_cooking.gif",
        "position": "bottom",
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_BABY_COOKING_SAD",
        "image": "assets/baby_cooking_sad.gif",
        "position": "bottom",
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_BABY_BURNING",
        "image": "assets/baby_burning.gif",
        "position": "bottom",
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_BABY_DEAD",
        "type": "dead",
        "image": "assets/baby_dead.gif",
        "position": "bottom",
        "backgroundImage": "assets/bg_flames.gif"
      },
      { "id": "BE_RAW_IDLE", "image": "assets/raw_idle.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_RAW_SAD", "image": "assets/raw_sad.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_RAW_ASLEEP", "image": "assets/raw_sleep.gif", "position": "bottom", "offsetY": -50 },
      {
        "id": "BE_RAW_FROZEN",
        "image": "assets/raw_frozen.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_frozen.gif"
      },
      {
        "id": "BE_RAW_COOKING",
        "image": "assets/raw_cooking.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_RAW_COOKING_SAD",
        "image": "assets/raw_cooking_sad.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_RAW_BURNING",
        "image": "assets/raw_burning.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_RAW_DEAD",
        "type": "dead",
        "image": "assets/raw_dead.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_flames.gif"
      },


      { "id": "BE_COOKED_IDLE", "image": "assets/cooked_idle.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_COOKED_SLEEP", "image": "assets/cooked_sleep.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_COOKED_DRESSED_IDLE", "image": "assets/dressed_idle.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_COOKED_DRESSED_SLEEP", "image": "assets/dressed_sleep.gif", "position": "bottom", "offsetY": -50 },

      { "id": "BE_DEAD", "type": "dead", "image": "assets/raw_dead.gif", "position": "bottom", "offsetY": -50 }
    ],
    "interactions": [
      {
        "id": "INT_GRILL",
        "label": "Grill",
        "cooldown": 0,
        "changeToggle": {
          "defaultState": "off",
          "onState": {
            "statusId": "STATUS_GRILL_ON",
            "statId": "temperature",
            "perMinute": 50.0
          },
          "offState": {
            "statusId": "STATUS_GRILL_OFF"
          }
        }
      },
      {
        "id": "INT_CLEAN",
        "label": "Clean",
        "cooldown": 5000,
        "changeStats": [
          {
            "statId": "waste",
            "value": -15
          }
        ],
        "hideWhenUnavailable": false,
        "availability": [
          {
            "when": ["STATUS_WASTE_DIRTY"],
            "then": true
          }
        ]
      },
      {
        "id": "INT_EVOLVE",
        "label": "Evolve!",
        "cooldown": 300,
        "availability": [
          {
            "when": ["STATUS_TEMP_GOLDENBROWN", "STATUS_WASTE_NORMAL"],
            "then": true
          }
        ],
        "hideWhenUnavailable": true,
        "changeStats": [
          {
            "statId": "level",
            "value": 1
          },
          {
            "statId": "temperature",
            "type": "absolute",
            "value": 0
          },
          {
            "statId": "waste",
            "type": "absolute",
            "value": 0
          }
        ]
      }
    ]
  }
}
