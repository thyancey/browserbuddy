        "offsetY": 0,
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_BABY_BURNING",
        "image": "assets/baby_burning.gif",
        "position": "bottom",
        "offsetY": 0,
        "backgroundImage": "assets/bg_flames.gif"
      },

      { "id": "BE_RAW_IDLE", "image": "assets/raw_idle.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_RAW_SAD", "image": "assets/raw_sad.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_RAW_ASLEEP", "image": "assets/raw_sleep.gif", "position": "bottom", "offsetY": -50 },
      {
        "id": "BE_RAW_FROZEN",
        "image": "assets/adult_frozen.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_frozen.gif"
      },
      {
        "id": "BE_RAW_COOKING",
        "image": "assets/raw_cooking.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_flames.gif"
      },
      {
        "id": "BE_RAW_BURNING",
        "image": "assets/hot_idle.gif",
        "position": "bottom",
        "offsetY": -50,
        "backgroundImage": "assets/bg_flames.gif"
      },

      { "id": "BE_COOKED_IDLE", "image": "assets/cooked_idle.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_COOKED_SLEEP", "image": "assets/cooked_sleep.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_COOKED_DRESSED_IDLE", "image": "assets/dressed_idle.gif", "position": "bottom", "offsetY": -50 },
      { "id": "BE_COOKED_DRESSED_SLEEP", "image": "assets/dressed_sleep.gif", "position": "bottom", "offsetY": -50 },

      { "id": "BE_DEAD", "image": "assets/hot_idle.gif", "position": "bottom", "offsetY": -50 }
    ],
    "toggles": [
      {
        "id": "TOGGLE_GRILLING",
        "on": "STATUS_GRILL_ON",
        "off": "",
